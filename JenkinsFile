pipeline{
    agent none
	stages{
		stage('Clone repository'){
			steps{
			    echo " checking out scm"
			    checkout scm
			}
		}
		stage('Build'){
// 			node HelloWorld.js
            steps{
                echo "building"
            }
		}
		stage('FInal'){
		    steps{
			    echo "Everything done"
		    }
		}
	}
}
