pipeline{
    agent none
	stages{
		stage('Clone repository'){
			steps{
			    echo " checking out scm"
			}
		}
		stage('Build'){
			steps{
                		echo "building"
				node HelloWorld.js
     			}
            	}
		stage('FInal'){
		    steps{
			    echo "Everything done"
		    }
		}
	}
}
